{
  "name": "lwr-project",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf __lwr_cache__",
    "start": "lwr serve --mode prod",
    "start:compat": "lwr serve --mode compat",
    "start:prod-compat": "lwr serve --mode prod-compat",
    "build": "npx tailwindcss -i ./src/main.scss -o ./dist/main.css",
    "watch": "npx tailwindcss -i ./src/main.scss -o ./dist/main.css --watch",
    "dev": "next dev"
  },
  "dependencies": {
    "@avonni/base-components": "^1.3.0",
    "lightning-base-components": "^1.14.7-alpha",
    "lwc": "2.5.8",
    "lwr": "0.6.5"
  },
  "devDependencies": {
    "@salesforce-ux/design-system": "^2.17.5",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "autoprefixer": "^10.4.7",
    "cpx": "^1.5.0",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "postcss": "^8.4.14",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-nesting": "^10.1.8",
    "postcss-preset-env": "^7.7.1",
    "postcss-scss": "^4.0.4",
    "precss": "^4.0.0",
    "sass": "^1.52.3",
    "tailwindcss": "^3.1.3"
  },
  "engines": {
    "node": ">=14.15.4 <17"
  }
}
